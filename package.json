{
	"name": "1337online",
	"version": "3.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"env:local": "printf VITE_ENVIRONMENT=local > .env",
		"env:uat": "printf VITE_ENVIRONMENT=uat > .env",
		"env:prod": "printf VITE_ENVIRONMENT=prod > .env",
		"start": "vite",
		"server": "php -S 127.0.0.1:1234",
		"build": "run-p type-check \"build-only {@}\" --",
		"build:local": "VITE_ENVIRONMENT=local npm run build && npm run dist:env:local",
		"build:uat": "VITE_ENVIRONMENT=uat npm run build && npm run dist:env:uat",
		"build:prod": "VITE_ENVIRONMENT=prod npm run build && npm run dist:env:prod",
		"dist:env:local": "printf VITE_ENVIRONMENT=local > dist/.env",
		"dist:env:uat": "printf VITE_ENVIRONMENT=uat > dist/.env",
		"dist:env:prod": "printf VITE_ENVIRONMENT=prod > dist/.env",
		"preview": "vite preview",
		"test:unit": "vitest",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"lint": "eslint . --fix",
		"format": "prettier --write src/"
	},
	"dependencies": {
		"axios": "1.7.9",
		"dayjs": "1.11.13",
		"vite-svg-loader": "5.1.0",
		"vue": "^3.5.12",
		"vue-router": "^4.4.5"
	},
	"devDependencies": {
		"@mdi/font": "7.4.47",
		"@tailwindcss/postcss": "^4.0.0-beta.8",
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.7",
		"@types/node": "22.10.5",
		"@vitejs/plugin-vue": "^5.1.4",
		"@vitest/eslint-plugin": "1.1.7",
		"@vue/eslint-config-prettier": "^10.1.0",
		"@vue/eslint-config-typescript": "^14.1.3",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.5.1",
		"eslint": "^9.14.0",
		"eslint-plugin-tailwindcss": "^3.17.5",
		"eslint-plugin-vue": "^9.30.0",
		"jsdom": "^25.0.1",
		"npm-run-all2": "^7.0.1",
		"prettier": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.8",
		"tailwindcss": "^4.0.0-beta.8",
		"typescript": "~5.6.3",
		"vite": "^5.4.10",
		"vite-plugin-static-copy": "^2.2.0",
		"vite-plugin-vue-devtools": "^7.5.4",
		"vitest": "^2.1.4",
		"vue-tsc": "^2.1.10"
	}
}
